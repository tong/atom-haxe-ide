{
    "name": "haxe-ide",
    "main": "./lib/haxe-ide",
    "version": "0.3.1",
    "description": "Haxe Development Environment",
    "keywords": ["haxe","ide","haxe-ide"],
    "consumedServices": {
        "status-bar": { "versions": { "^1.0.0": "consumeStatusBar" } }
    },
    "providedServices": {
        "haxe-server": {
            "description": "Provides haxe build/completion server",
            "versions": {
                "0.1.0": "provideServerService"
            }
        },
        "haxe-build": {
            "description": "Provides haxe build service",
            "versions": {
                "0.1.0": "provideBuildService"
            }
        },
        "haxe-completion": {
            "description": "Provides haxe completion service",
            "versions": {
                "0.1.0": "provideCompletionService"
            }
        },
        "autocomplete.provider": {
            "versions": {
                "2.0.0": "provideAutoCompletion"
            }
        }
    },
    "repository": "https://github.com/tong/atom-haxe-ide",
    "license": "GPL",
    "engines": {
        "atom": ">=1.4.0 <2.0.0"
    },
    "dependencies": {
        "language-haxe": "^0.3.0"
    }
}
