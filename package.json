{
    "name": "xxx",
    "version": "0.9.5",
    "description": "Sex with haxe",
    "keywords": ["haxe","ide","xxx"],
    "repository": "https://github.com/tong/atom-xxx",
    "license": "MIT",
    "engines": {
        "atom": ">=1.20.0 .0 <2.0.0"
    },
    "dependencies": {
        "language-haxe": "^0.5.3"
    },
    "main": "./lib/xxx",
    "configSchema": {
        "haxe_path": {
            "order": 1,
            "title": "Haxe Path",
            "description": "Path to your haxe executable file",
            "type": "string",
            "default": "haxe"
        },
        "haxe_server_startdelay": {
            "order": 2,
            "title": "Server Activation Delay",
            "description": "The delay in seconds before starting the haxe server.",
            "type": "integer",
            "minimum": 0,
            "maximum": 5,
            "default": 1
        },
        "hxml_search_depth": {
            "order": 3,
            "title": "Hxml Search Depth",
            "description": "Filesystem depth to search for hxml files",
            "type": "integer",
            "minimum": 0,
            "maximum": 10,
            "default": 2
        },
        "autocomplete": {
            "order": 4,
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean",
                    "default": true
                }
            }
        }
    },
    "consumedServices": {
        "status-bar": { "versions": { "^1.1.0": "consumeStatusBar" } }
    },
    "providedServices": {
        "autocomplete.provider": {
            "versions": {
                "2.0.0": "provideAutoCompletion"
            }
        },
        "haxe": {
            "description": "Provides access to haxe compiler",
            "versions": {
                "0.1.0": "provideService"
            }
        }
    }
}
